pipeline {
    agent any
   stages {
        stage('Docker Build') {
            steps {
                sh 'cd LMS-BE && docker build -t backend .'
            }
        }        
        stage('Docker container') {
            steps {
                sh 'docker container run -dt --name be --network lmsnetwork -p 8181:8080 backend'
            }
        }
    }
}
